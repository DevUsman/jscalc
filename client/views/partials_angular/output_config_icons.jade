md-button.md-icon-button(ng-if='editMode', aria-label='Configure', ng-click="configureOutput({metaOutputs: doc.metaOutputs, metaOutput: metaOutput})")
  md-icon(md-svg-icon="/img/icons/ic_settings_24px.svg")
md-button.md-icon-button(ng-if='editMode', aria-label='Delete', ng-click="deleteOutput({id: metaOutput.id})")
  md-icon(md-svg-icon="/img/icons/ic_delete_24px.svg")
.hidden-indicator.fade-in(ng-if="outputAutohidden(metaOutput)")
  .content Hidden
